 "css": ".hr{ \r\n    margin-top: 7px; \r\n    margin-bottom: 7px; \r\n} \r\n.print-format{ \r\n    font-size: 8pt;\r\n    padding: 30px !important;\r\n} \r\n.print-format td, .print-format th{ \r\n    padding: 3px !important; \r\n}\r\n\r\n @media print {\r\n    @page {\r\n        size: A4;\r\n        margin: 20mm;\r\n    }\r\n    .page-break {\r\n        page-break-before: always;\r\n    }\r\n    .last-page footer {\r\n        position: relative;\r\n        bottom: 0;\r\n        display: table-footer-group; \r\n    }\r\n    body {\r\n        font-size: 12px;\r\n    }\r\n}\r\n\r\nfooter {\r\n    clear: both;\r\n    position: relative;\r\n    height: 200px;\r\n    margin-top: -200px;\r\n}\r\n",
