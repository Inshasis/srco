 "doctype": "Print Format",
 "font_size": 14,
 "html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <link rel=\"stylesheet\" href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css\">\n    <script src=\"https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js\"></script>\n    <script src=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js\"></script>\n    <style>\n        table {\n          font-family: arial, sans-serif;\n          border-collapse: collapse;\n        }\n        td, th {\n          border-top: 1px solid black;\n          text-align: left;\n        }\n        .tablee td, th {\n            width: 0%;\n        }\n        </style>    \n</head>\n<body >\n    \n\t<div class=\"letter\" style=\"margin:-30px;\">\n\t    <div class=\"header\">\n                <img src=\"/files/letterpad.png\">\n                <br>\n                <!--<center><img src=\"/files/logo.png\" style=\"width: 45%;padding: 15px !important;\"></center>-->\n                {% set tax_id = frappe.db.get_value('Company', {'name':doc.company}, ['tax_id']) %}\n                <center><p style=\"font-size:15px;font-weight: 700;float: center;padding-top: 15px !important;margin-top:-15px;\">Proforma Invoice</p></center>\n                <div class=\"main2\" style=\"font-weight: 600;width: 98%;float: left;padding-left: 20px;padding-top: 0px !important;\">\n                    <div class=\"pra5\" style=\"width: 50%; float: left;font-weight: 600;padding-bottom:-10px;border-top:1px solid;border-right:1px solid;border-left:1px solid;padding-left: 10px;\">\n                        <h5 style=\"font-size: 14px;\"><b></b></h5>\n                        <p style=\"font-size:12px;font-weight: 700;\">Customer Name : {{doc.customer_name}} ({{doc.customer}})</p>\n                        <p style=\"font-size:12px;font-weight: 700;\">SO NO : {{doc.name}}</p>\n                        <p style=\"font-size:12px;font-weight: 600;\">PO No: {{doc.po_no}}</p>\n                        <p style=\"font-size:13px;\">Bill To: </p>\n                        {% if doc.customer_address %}\n                        {% set address_line1, address_line2, city, country, state = frappe.db.get_value('Address', {'name':doc.customer_address}, ['address_line1','address_line2','city','country','state']) %}\n                        <p style=\"font-size:11px;\">{{address_line1 or ''}}, {{address_line2 or ''}}, {{city or ''}}, {{state or ''}},{{country or ''}} </p>\n                        \n                        {% else %}\n                        <p style=\"font-size:11px;\"></p>\n                        {% endif %}\n                        <p style=\"font-size:10px;height:6px;\"></p>\n                        \n                    </div>\n                    \n                    <div class=\"pra5\" style=\"width: 50%; float: left;font-weight: 600;padding-bottom:-10px;border-top:1px solid;border-right:1px solid;padding-left: 10px;\">\n                        <h5 style=\"font-size: 14px;\"><b></b></h5>\n                       <br>\n                        <p style=\"font-size:12px;font-weight: 700;\">SO Date : {{doc.name}}</p>\n                        <p style=\"font-size:12px;font-weight: 700;\">PO Date: {{doc.po_date}}  </p>\n                        <p style=\"font-size:13px;\">Ship To: </p>\n                        {% if doc.shipping_address_name %}\n                        {% set address_line1, address_line2, city, country, state = frappe.db.get_value('Address', {'name':doc.customer_address}, ['address_line1','address_line2','city','country','state']) %}\n                        <p style=\"font-size:11px;\">{{address_line1 or ''}}, {{address_line2 or ''}}, {{city or ''}}, {{state or ''}},{{country or ''}} </p>\n                        \n                        {% else %}\n                        <p style=\"font-size:11px;\"></p>\n                        {% endif %}\n                        <p style=\"font-size:10px;height:27px;\"></p>\n                        \n                    </div>\n                </div>\n                \n              \n                <div class=\"main4\">\n                    <table class=\"table\" style=\"border-top: 1px solid black;border-bottom: 1px solid black;border-left: 1px solid black;width: 95.5%;margin-left: 20px;margin-top: !important;padding: 48px;\">\n                        <thead>\n                        <tr style=\"background-color: lightgray;font-size:11px;\">\n                            <th style=\"border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:4%;text-align: center;\"><b>SN</b></th>\n                            <!--<th style=\"border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:7%;text-align: center;\"><b>LPO SN</b></th>-->\n                             <th style=\"border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:43.5%;text-align: left;\"><b>Product Description</b></th>\n                            <!--<th style=\"border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:25%;text-align: left;\"><b>Other Details</b></th>-->\n\n                            <th style=\"border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:6%;text-align: center;\"><b>UOM</b></th>\n                            <th style=\"border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:8%;text-align: center;\"><b>Qty</b></th>\n                            <th style=\"border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:10%;text-align: center;\"><b>Rate</b></th>\n                            <th style=\"border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:12%;text-align: center;\"><b>Total Amount</b></th>\n                        </tr>\n                    </thead>\n                    <tbody>\n                        {% for i in doc.items %}\n                        <tr style=\"height: 27px;\">\n                            <td style=\"border-right:1px solid black ;border-bottom:1px solid black;text-align: center;\">{{i.idx}}</td>\n                            <!--<td style=\"border-right:1px solid black ;border-bottom:1px solid black;text-align: center;\">{{i.custom_lpo_sn or ''}}</td>-->\n                            <td style=\"border-right:1px solid black ;border-bottom:1px solid black;text-align: left;\">{{i.item_name}} <span style=\"color:blue\">( {{i.item_code}} )</span></td>\n                            <!--<td style=\"border-right:1px solid black ;border-bottom:1px solid black;text-align: left;\"><b></b></td>-->\n                            <td style=\"border-right:1px solid black ;border-bottom:1px solid black;text-align: center;\">{{i.uom}}</td>\n                            <td style=\"border-right:1px solid black ;border-bottom:1px solid black;text-align: center;\">{{i.qty}}</td>\n                            <td style=\"border-right:1px solid black ;border-bottom:1px solid black ;text-align: center;\">{{\"%.3f\"|format(i.rate) }}</td>\n                            <td style=\"border-right:1px solid black ;border-bottom:1px solid black ;text-align: center;\"\">{{\"%.3f\"|format(i.amount) }}</td>\n                        </tr>\n                        {% endfor %}\n                    </tbody>\n                </table>\n                \n            </div>\n            \n                <div class=\"main11d2\" style=\"width: 100%; float: left; padding-left: 20px; padding-top: -45px;margin-bottom:20px;\">\n                    <div class=\"prda5\" style=\"padding: 15px; width: 98%; float: right; font-weight: 700; border: 1px solid black; border-top: none; margin-top: -20px; margin-right: 16px;\">\n                        <p style=\"font-size: 12px; margin-left: 150px; padding-bottom: 6px;float: right;\">\n                            Total Amount : <b style=\"margin-left: 10px;\">{{ \"%.3f\"|format(doc.total) }}</b> <br>\n                             <b style=\"margin-left: 25px;\">Discount : <span style=\"margin-left: 10px;\">{{ \"%.3f\"|format(doc.discount_amount) }}</span></b> <br>\n                             <!--<b style=\"margin-left: 25px;\">VAT (5%) : <span style=\"margin-left: 10px;\">{{ \"%.3f\"|format(doc.total_taxes_and_charges) }}</span></b> <br>-->\n                             <b style=\"margin-left: 8px;\">Net Amount : <span style=\"margin-left: 10px;\">{{ \"%.3f\"|format(doc.grand_total) }}</span></b>\n                        </p>\n                        <div style=\"text-align: center; width: 100%; margin-top: 20px;\">\n                          <p style=\"font-size: 11px; font-weight: 700; \n                                    margin: 0; padding: 0 10px; \n                                    text-align: center; \n                                    max-width: 100%; \n                                    word-wrap: break-word; \n                                    overflow-wrap: break-word; \n                                    hyphens: auto; \n                                    line-height: 1.2;\">\n                            &nbsp;&nbsp;{{ doc.in_words }}\n                          </p>\n                        </div>\n                    </div>\n                </div>\n                \n                \n                {% set user_name, mobile, email = frappe.db.get_value('User', {'name':frappe.session.user}, ['full_name','mobile_no','email']) %}\n            <div style=\"margin-left:25px;margin-top:-15px;font-size:11px;font-weight:700;\">\n                {% if doc.party %}\n                {% set account_name,bank,branch_code,bank_account_no,custom_swift_code,finance_code,custome_tax_card = frappe.db.get_value('Bank Account', {'name':doc.party}, ['account_name','bank','branch_code','bank_account_no','custom_swift_code','custom_finance_code','custom_tax_card']) %}\n                    <p style=\"font-size:14px\">Bank Details : </p>\n                    <p>Bank Name : {{bank or \"\"}} / IBAN No : {{branch_code or \"\"}} / SWIFT Code : {{custom_swift_code or \"\"}}</p>\n                    <p>Bank Account Number : {{bank_account_no or \"\"}} </p>\n                    <p>Finance Code : {{finance_code or \"\"}}</p>\n                    <p>Tax card Id: {{custome_tax_card or \"\"}}</p>\n                {% else %}\n                    <p style=\"font-size:14px\">Bank Details : </p>\n                    <p>Bank Name : </p>\n                    <p>Bank Account Number : </p>\n                    <p>Finance Code : </p>\n                    <p>Tax card Id : </p>\n                {% endif %}\n                <br>\n            \n                <br>\n                <center><b style=\"padding-bottom: 20px;\">Issued By : {{user_name or ''}} / {{mobile or ''}} / {{email or ''}}</b></center>\n            </div>\n                \n                \n        </div>\n    </div>\n</body>\n</html>",
